<?xml version="1.0" encoding='utf-8'?>
<theme name='Light and White panel' background="">

  <color name='normal'>     #  FFFFFF</color>
  <color name='contrast'>   #  FFEEEE</color>
  <color name='minor'>      #  505050</color>
  <color name='subtle'>     #  303030</color>
  <color name='back'>       #  000000</color>
  
  <font-size name='small'  value='10'/>
  <font-size name='normal' value='12'/>
